{% extends '::base.html.twig' %}
<h1>Ajouter un étudiant</h1>

{% if message %}
    <p>{{ message }}</p>
{% endif %}

<form action="" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <input type="submit" />
</form>
<p><a href="{{ path('listPreinscrit') }}">Retour à la liste des étudiants</a></p>