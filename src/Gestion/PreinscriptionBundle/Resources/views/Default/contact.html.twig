{# contact.html.twig #}
{% extends "layoutAdmin.html.twig" %}

{% block body %}
    <div class="col-md-12 profile-contain">
    <div class="row">
    <div class="col-md-12 col-xs-12">
    <div class="content">
{{ form_start(form) }}

    <div class="form-group">
        <label for="contact_subject">Sujet</label><br>
        {{ form_widget(form.subject) }}
    </div>

    <div class="form-group">
        <label for="contact_name">Nom</label><br>
        {{ form_widget(form.nom) }}
    </div>

    <div class="form-group">
        <label for="contact_prename">Pr√©nom</label><br>
        {{ form_widget(form.prenom) }}
    </div>

    <div class="form-group">
        <label for="contact_email">{{ form_label(form.email) }}</label><br>
        {{ form_widget(form.email) }}
    </div>

    <div class="form-group">
        <label for="contact_message">{{ form_label(form.message) }}</label><br>
        {{ form_widget(form.message) }}
    </div>

    {{ form_rest(form) }}

<input type="submit" class="btn btn-success" value="Envoyer" >

{{ form_end(form) }}
    </div></div></div></div>

{% endblock %}